{
    "model": {
        "type": "range_dispatch",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "minecraft:item/trident"
        },
        "entries": [
            {
                "threshold": 1,
				"model": {
					"type": "minecraft:condition",
					"property": "minecraft:using_item",
					"on_true": {
						"type": "model",
						"model": "item/gondor/spear/gondor_spear_throw"
					},
					"on_false": {
						"type": "model",
						"model": "item/gondor/spear/gondor_spear"
					}
				}
            }
        ]
    }
}